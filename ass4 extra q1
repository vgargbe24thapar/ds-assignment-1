#include <iostream>
#include <queue>
using namespace std;

void generateBinaryNumbers(int n) {
    // Time Complexity: O(n * log n) approximately, 
    // because generating binary for each number involves string operations of length ~log n
    // Space Complexity: O(n * log n) for storing the queue elements as strings

    queue<string> q;
    q.push("1");  // Start with binary 1

    for (int i = 1; i <= n; i++) {
        string s = q.front();
        q.pop();

        cout << s;
        if (i != n) cout << ", ";

        // Append "0" and "1" to current string and enqueue them
        q.push(s + "0");
        q.push(s + "1");
    }
}

int main() {
    int n = 2;
    cout << "Binary numbers from 1 to " << n << ": ";
    generateBinaryNumbers(n);
    cout << endl;
    return 0;
}
